<% content_for :dashboard_content do %>
  <div class="card">
    <div class="card-header card-header-primary">
      <p id="notice"><%= notice %></p>
      <h3>Achievements</h3>
    </div>
    <div class="card-body">
      <div id="notice">
        <% flash.each do |class_name, message| %>
          <div class="alert alert-<%= class_name %>"><%= message %></div>
        <% end %>
      </div>
      <%= link_to 'Add achievement', new_admin_achievement_path, class: "btn btn-primary" %>
      <table id="achievements-dt" data-source="<%= admin_achievements_path %>" width="100%" class="table table-striped table-bordered">
        <thead>
          <tr>
            <th>Title</th>
            <th>User</th>
            <th>Status</th>
            <th></th>
          </tr>
        </thead>
        <tbody></tbody>
      </table>
    </div>
  </div>
<% end %>
<%= render template: 'layouts/material_dashboard' %>
