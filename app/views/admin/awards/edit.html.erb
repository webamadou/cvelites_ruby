<%= content_for :dashboard_content do %>
  <div class="card">
    <div class="card-header card-header-primary mb-3">
      <h3>Editing Admin Award</h3>
    </div>
    <div class="card-body">
      <%= render 'form', admin_award: @admin_award %>

      <%= link_to 'Show', @admin_award, class: "btn btn-success" %> |
      <%= link_to 'Back', admin_awards_path, class: "btn btn-default" %>
    </div>
  </div>
<% end %>
<%= render template: 'layouts/material_dashboard' %>