<p id="notice"><%= notice %></p>

<h1>Admin Awards</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Title</th>
      <th>Description</th>
      <th>Date</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @admin_awards.each do |admin_award| %>
      <tr>
        <td><%= admin_award.user %></td>
        <td><%= admin_award.title %></td>
        <td><%= admin_award.description %></td>
        <td><%= admin_award.date %></td>
        <td><%= link_to 'Show', admin_award %></td>
        <td><%= link_to 'Edit', edit_admin_award_path(admin_award) %></td>
        <td><%= link_to 'Destroy', admin_award, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Admin Award', new_admin_award_path %>
