<%= content_for :dashboard_content do %>
  <div class="card">
    <div class="card-header card-header-primary">
      <p id="notice"><%= notice %></p>
      <h3>Admin Educations</h3>
    </div>
    <div class="card-body">
      <table id="educations-dt" class="table table-striped table-bordered" width="100%" data-source="<%=  admin_educations_path %>">
        <thead>
          <tr>
            <th>School</th>
            <th>Degree</th>
            <th>Graduation year</th>
            <th>Description</th>
            <th>Address</th>
            <th></th>
          </tr>
        </thead>
        <tbody></tbody>
      </table>
      <br>
      <%= link_to 'New Admin Education', new_admin_education_path %>
    </div>
  </div>
<% end %>
<%= render template: 'layouts/material_dashboard' %>