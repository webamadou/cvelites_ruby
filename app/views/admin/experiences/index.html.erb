<p id="notice"><%= notice %></p>

<h1>Admin Experiences</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Company</th>
      <th>User</th>
      <th>Description</th>
      <th>Started at</th>
      <th>Ended at</th>
      <th>Still there</th>
      <th>Status</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @admin_experiences.each do |admin_experience| %>
      <tr>
        <td><%= admin_experience.title %></td>
        <td><%= admin_experience.company %></td>
        <td><%= admin_experience.user %></td>
        <td><%= admin_experience.description %></td>
        <td><%= admin_experience.started_at %></td>
        <td><%= admin_experience.ended_at %></td>
        <td><%= admin_experience.still_there %></td>
        <td><%= admin_experience.status %></td>
        <td><%= link_to 'Show', admin_experience %></td>
        <td><%= link_to 'Edit', edit_admin_experience_path(admin_experience) %></td>
        <td><%= link_to 'Destroy', admin_experience, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Admin Experience', new_admin_experience_path %>
