<%= content_for :dashboard_content do %>
  <div class="card">
    <div class="card-header card-header-primary">
      <p id="notice"><%= notice %></p>
      <strong>Language</strong>
    </div>
    <div class="card-body">
      <br>
      <%= link_to 'New Admin Language', new_admin_language_path, class: 'btn btn-primary' %>
      <table id="languages-dt" class="table table-bordered table-striped" width="100%" data-source="<%= admin_languages_path(format: :json) %>">
        <thead>
          <tr>
            <th>Name</th>
            <th>Code</th>
            <th>Status</th>
            <th></th>
          </tr>
        </thead>
        <tbody></tbody>
      </table>
    </div>
  </div>
<% end %>
<%= render template: 'layouts/material_dashboard' %>