<p id="notice"><%= notice %></p>

<h1>Admin Resumes</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>User</th>
      <th>Description</th>
      <th>Status</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @admin_resumes.each do |admin_resume| %>
      <tr>
        <td><%= admin_resume.name %></td>
        <td><%= admin_resume.user %></td>
        <td><%= admin_resume.description %></td>
        <td><%= admin_resume.status %></td>
        <td><%= link_to 'Show', admin_resume %></td>
        <td><%= link_to 'Edit', edit_admin_resume_path(admin_resume) %></td>
        <td><%= link_to 'Destroy', admin_resume, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Admin Resume', new_admin_resume_path %>
